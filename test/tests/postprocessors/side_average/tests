[Tests]
    [postprocessor_test]
        [setup]
            type = RunCommand
            command = 'bash -c "foamCleanCase -case foam && blockMesh -case foam && decomposePar -case foam"'
        []
        [check_csv]
            type = CSVDiff
            input = main.i
            prereq = postprocessor_test/setup
            csvdiff = main_out.csv
            allow_warnings = true
        []
        [check_csv_parallel]
            type = CSVDiff
            input = main.i
            prereq = postprocessor_test/setup
            csvdiff = main_out.csv
            allow_warnings = true
            min_parallel = 2
            max_parallel = 2
        []
        [boundary_err]
            type = RunException
            input = main.i
            prereq = postprocessor_test/setup
            allow_warnings = true
            cli_args='Postprocessors/t_avg/block=top1'
            expect_err = "do not exist on the mesh: top1"
        []
        [invalid_foam_scalar]
            type = RunException
            input = main.i
            prereq = postprocessor_test/setup
            allow_warnings = true
            cli_args='Postprocessors/t_avg/foam_variable=T1'
            expect_err = "No Foam scalar or function object called 'T1'."
        []
    []
[]
