{
    patch0
    {
        type            epsilonWallFunction;
        value           uniform 4e-06;
    }
    patch1
    {
        type            epsilonWallFunction;
        value           uniform 4e-06;
    }
    patch2
    {
        type            epsilonWallFunction;
        value           uniform 4e-06;
    }
    patch3
    {
        type            epsilonWallFunction;
        value           uniform 4e-06;
    }
    patch4
    {
        type            epsilonWallFunction;
        value           uniform 4e-06;
    }
    patch5
    {
        type            epsilonWallFunction;
        value           uniform 4e-06;
    }
}


