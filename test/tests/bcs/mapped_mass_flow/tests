            min_parallel = 5
            max_parallel = 5
        []
        [reconstruct]
            type = RunCommand
            command = 'reconstructPar -case foam'
            prereq = mapped_mass_flow/run
        []
        [verify]
            type = PythonUnitTest
            input = test.py
            prereq = mapped_mass_flow/reconstruct
        []
    []
[]
