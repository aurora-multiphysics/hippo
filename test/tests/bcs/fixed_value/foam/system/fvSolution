FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    rho
    {
        solver          diagonal;
    }

    rhoFinal
    {
        $rho;
    }


    e
    {
        solver           PBiCGStab;
        preconditioner   DIC;
        tolerance        1e-8;
        relTol           1e-8;
    }

    eFinal
    {
        $e;
        tolerance        1e-8;
        relTol           1e-8;
    }
}

PIMPLE
{
    momentumPredictor   yes;
    pRefCell            0;
    pRefValue           0;
}

relaxationFactors
{
    equations
    {
        h               1;
        U               1;
    }
}

