FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIsoSolid;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       1;
    }
    thermodynamics
    {
        Cv              16;        // Specific heat capacity [J/(kg·K)]
        Hf              0;         // Heat of formation [J/kg]
    }
    transport
    {
        kappa           4;         // Thermal conductivity [W/(m·K)]
        mu              1e-3;      // Dynamic viscosity [Pa·s]
    }
    equationOfState
    {
        rho             1;         // Density [kg/m^3]
    }
}


